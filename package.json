{
    "name": "bbog-dig-angular-template-web-ui",
    "version": "0.0.1",
    "description": "Template example for generating frontend repos for bdb. Courtesy of @mnieto2",
    "keywords": [
        "github",
        "actions",
        "angular",
        "husky",
        "bbog",
        "template"
    ],
    "bugs": {
        "email": "mnieto2@bancodebogota.com.co",
        "url": "https://github.com/bancodebogota/bbog-dig-angular-template-web-ui/issues"
    },
    "author": {
        "email": "mnieto2@bancodebogota.com.co",
        "name": "Mateo Nieto",
        "url": "https://github.com/mnieto2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bancodebogota/bbog-dig-angular-template-web-ui.git"
    },
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "dev": "ng serve --port $PORT",
        "build": "ng build",
        "build:qa": "ng build --configuration=qa --aot",
        "build:st": "ng build --configuration=stage --aot",
        "build:prod": "ng build --configuration=production --prod",
        "test": "ng test --browsers ChromeHeadless",
        "coverage": "ng test --browsers ChromeHeadless --codeCoverage=true",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "sonar": "gulp sonar"
    },
    "husky": {
        "//1": "Use git commit --no-verify to bypass the pre-commit hook",
        "//2": "Use git push --no-verify to bypass the pre-push hook",
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run test"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.1.9",
        "@angular/common": "9.1.9",
        "@angular/compiler": "9.1.9",
        "@angular/core": "9.1.9",
        "@angular/elements": "^9.1.9",
        "@angular/forms": "9.1.9",
        "@angular/platform-browser": "9.1.9",
        "@angular/platform-browser-dynamic": "9.1.9",
        "@angular/router": "9.1.9",
        "document-register-element": "^1.7.2",
        "rxjs": "~6.5.5",
        "tslib": "^1.10.0",
        "zone.js": "^0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.3",
        "@angular/cli": "9.1.7",
        "@angular/compiler-cli": "9.1.9",
        "@angular/language-service": "9.1.9",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "husky": "^4.2.5",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typescript": "3.8.3"
    }
}
